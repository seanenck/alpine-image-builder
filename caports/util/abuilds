#!/bin/sh -e

{
  echo "# do NOT edit, generated: $(date +%Y-%m-%dT%H:%M:%S)"
  cat << EOF
PACKAGER="Sean Enck <sean@ttypty.com>"
REPOROOT="/home/enck/Env/guestos/cusp/packages"
REPODEST="/home/enck/Env/guestos/cusp/packages/cusp/next"
PACKAGER_PRIVKEY=/home/enck/.abuild/dev@cusplinux.org-674b2f71.rsa
EOF
} > "$HOME/.abuild/abuild.conf"
